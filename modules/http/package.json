{
  "$schema": "https://json.schemastore.org/package",
  "name": "@ocean/http",
  "version": "0.0.5",
  "description": "@ocean/http",
  "main": "dist/index.cjs.js",
  "typings": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "ocean",
    "http"
  ],
  "author": "OceanCommon",
  "license": "MIT",
  "publishConfig": {
    "registry": "http://never.aims.nevermonarch.cn:64873",
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "version": "node ../../scripts/version.js",
    "check": "tsc --noEmit",
    "build": "npm run clean && npm run check && vite build -c ../../vite.config.ts",
    "test": "pnpm run build & node ./dist/index.js",
    "release": "pnpm run build && pnpm run version --patch && pnpm publish --no-git-checks",
    "unrel": "pnpm unpublish --force",
    "rel": "pnpm publish"
  },
  "dependencies": {
    "@ocean/http": "file:",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "mongodb": "^6.16.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/mongodb": "^4.0.7"
  }
}
