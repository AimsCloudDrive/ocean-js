{
  "$schema": "https://json.schemastore.org/package",
  "name": "@msom/common",
  "version": "0.0.2",
  "description": "@msom/common",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "msom",
    "common"
  ],
  "author": "msom",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "version": "node ../../scripts/version.js",
    "check": "tsc --noEmit",
    "build": "npm run clean && npm run check && vite build -c ../../vite.config.ts",
    "release": "pnpm run build && pnpm run version --patch && pnpm publish --no-git-checks",
    "unrel": "pnpm unpublish --force",
    "rel": "pnpm publish",
    "dev": "pnpm run -r --filter=./example dev"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@msom/common": "file:"
  }
}
