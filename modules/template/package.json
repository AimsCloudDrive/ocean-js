{
  "$schema": "https://json.schemastore.org/package",
  "name": "@msom/template",
  "version": "1.0.1",
  "description": "@msom/template",
  "main": "dist/index.js",
  "typings": "dist/types/index.d.ts",
  "type": "module",
  "files": [
    "bin",
    "dist",
    "README.md"
  ],
  "keywords": [
    "msom",
    "template"
  ],
  "author": "msom",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "version": "node ../../scripts/version.js",
    "check": "tsc --noEmit",
    "compile": "xbuild build -C -c ./xbuild.config.ts",
    "build::vite": "pnpm run clean && pnpm run check && vite build -c ./vite.config.ts",
    "build": "npm run clean && pnpm run check && xbuild build -c ./xbuild.config.ts",
    "release": "pnpm run build && pnpm run version --patch && pnpm publish --no-git-checks"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@msom/dom": "workspace:^",
    "@msom/common": "workspace:^",
    "@msom/component": "workspace:^",
    "@msom/reaction": "workspace:^"
  },
  "devDependencies": {}
}
